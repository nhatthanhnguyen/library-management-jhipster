        <addForeignKeyConstraint baseColumnNames="book_copy_id"
                                 baseTableName="checkout"
                                 constraintName="fk_checkout__book_copy_id"
                                 referencedColumnNames="id"
                                 referencedTableName="book_copy"/>
    </changeSet>
</databaseChangeLog>
